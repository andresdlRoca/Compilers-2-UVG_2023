class Point inherits Object {
    x : Int;
    y : Int;
    init(g : Int, z : Int) : SELF_TYPE {
        
            self.x <- x;
            self.y <- y;
            self;
    };
};

class Cirlce inherits Point {

    radius : Int;
    init2(x: Int, y : Int, r : Int) : SELF_TYPE {
        self.x <- x;
        self.y <- y;
        self.radius <- r;
        self;
    };
};
